<% include partials/headerAdmin %>

<div class="container text-center mt-5">

    <h1> Ostud </h1>

    <table class="table table-hover table-bordered mt-4 mb-5">
  	<thead>
    	    <tr>
      		<th scope="col">#</th>
      		<th scope="col">Aeg</th>
      		<th scope="col">Ostja</th>
      		<th scope="col">Toode</th>
      		<th scope="col">Kogus</th>
		<th scope="col">Summa</th>
    	    </tr>
  	</thead>
  	<tbody>
	    <% ostud.forEach(function(ost, i) { %>
	    	<tr>
      		    <th scope="row"> <%= i + 1 %> </th>
      		    <td> <%= ost.aeg %> </td>
      		    <td> <%= ost.nimi %> </td>
      		    <td> <%= ost.nimetus %> </td>
		    <td> <%= ost.kogus %> </td>
 		    <td> <%= ost.summa.toFixed(1) %>â‚¬ </td>
           	</tr>
    	    <% }) %>
  	</tbody>
    </table>

</div>

<% include partials/footer %>